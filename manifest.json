{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "LoL Jungle Assistant",
    "version": "0.1.0",
    "minimum-overwolf-version": "0.199.0.7",
    "minimum-gep-version": "128.0.0.1",
    "description": "Master the jungle with intelligent camp tracking, strategic recommendations, and performance analytics. Educational tool for jungle improvement.",
    "author": "minelearnai",
    "dock_button_title": "Jungle Assistant",
    "icon": "assets/icons/icon.png",
    "icon_gray": "assets/icons/icon-gray.png",
    "launcher_icon": "assets/icons/launcher.png",
    "window_icon": "assets/icons/icon.png"
  },
  "permissions": [
    "GameInfo",
    "Hotkeys", 
    "FileSystem",
    "Profile"
  ],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [5426]
    },
    "game_events": [5426],
    "launch_events": [
      {
        "event": "GameLaunched",
        "event_data": {
          "game_ids": [5426]
        },
        "start_minimized": false
      }
    ],
    "start_window": "main",
    "windows": {
      "main": {
        "file": "dist/renderer/app.js",
        "show_in_taskbar": true,
        "transparent": false,
        "resizable": true,
        "size": {
          "width": 420,
          "height": 360
        },
        "min_size": {
          "width": 350,
          "height": 280
        }
      },
      "overlay": {
        "file": "dist/renderer/overlay.js",
        "transparent": true,
        "clickthrough": false,
        "resizable": false,
        "in_game_only": true,
        "size": {
          "width": 320,
          "height": 180
        }
      },
      "dashboard": {
        "file": "dist/renderer/dashboard.js",
        "show_in_taskbar": true,
        "resizable": true,
        "size": {
          "width": 1200,
          "height": 800
        }
      }
    },
    "hotkeys": {
      "toggle_overlay": {
        "title": "Toggle Jungle Overlay",
        "action-type": "toggle",
        "default": "Ctrl+Shift+J"
      }
    }
  }
}