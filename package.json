{
  "name": "lol-jungle-assistant",
  "productName": "LoL Jungle Assistant",
  "version": "0.1.0",
  "description": "Jungle-focused decision assistant for League of Legends with Overwolf integration",
  "main": "dist/main/main.js",
  "author": {
    "name": "minelearnai",
    "email": "minelearnai@example.com"
  },
  "license": "MIT",
  "overwolf": {
    "packages": [
      "gep",
      "overlay"
    ]
  },
  "scripts": {
    "start": "ow-electron .",
    "start:electron": "electron .",
    "dev": "concurrently \"npm:build:watch\" \"npm:start:electron\"",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "webpack --config webpack.renderer.js --mode production",
    "build:ow-electron": "ow-electron-builder",
    "build:watch": "npm run build -- --watch"
  },
  "devDependencies": {
    "@overwolf/ow-electron": "^37.2.6",
    "@overwolf/ow-electron-builder": "^25.0.7",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/yaml": "^1.9.6",
    "concurrently": "^8.2.2",
    "css-loader": "^6.11.0",
    "electron": "^28.2.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@overwolf/overwolf-api-ts": "^1.0.15",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "yaml": "^2.8.1"
  }
}
